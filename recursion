public class Main
{
	public static int MECH(int a1)
	{
	    if(a1==0)
	    return 0;
	    
	    System.out.println("MECH:"+a1);
	    IT(a1-1);
	    return 0;
	}
	
	
	public static int IT(int a)
	{
	    if(a==0)
	    return 0;
	    System.out.println("IT:"+a);
	    MECH(a-1);
	    return 0;
	}
	public static void main(String[] args) 
	{
	 	  MECH(5);
	}
}
